{
  "extends": ["eslint-config-carbon"],
  "plugins": ["react-hooks"],
  "rules": {
    "react/prop-types": 0,
    "react/forbid-prop-types": 0,
    "arrow-body-style": 0
  },
  "globals": {
    "fathom": "readonly"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": "src"
      }
    }
  }
}
